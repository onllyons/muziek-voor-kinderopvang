{
  "expo": {
    "name": "Muziek Kids",
    "slug": "muziek-voor-kinder",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "updates": {
      "url": "https://u.expo.dev/7c738b7b-b225-421a-b487-74fc065bb113",
      "enabled": true,
      "fallbackToCacheTimeout": 3000,
      "checkAutomatically": "ON_LOAD",
      "requestHeaders": {
        "expo-channel-name": "production"
      }
    },
    "ios": {
      "bundleIdentifier": "com.tuacompany.muziekvoorkinder",
      "supportsTablet": true,
      "buildNumber": "7",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Deze app gebruikt de camera niet actief, maar bevat modules die cameratoegang kunnen aanvragen.",
        "NSPhotoLibraryUsageDescription": "Deze app gebruikt de fotobibliotheek niet actief, maar bevat modules die toegang kunnen aanvragen.",
        "NSMicrophoneUsageDescription": "Deze app gebruikt de microfoon niet actief, maar bevat modules die toegang kunnen aanvragen.",
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "package": "com.tuacompany.muziekvoorkinder",
      "versionCode": 7,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "blockedPermissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7c738b7b-b225-421a-b487-74fc065bb113"
      }
    },
    "owner": "saschavan",
    "runtimeVersion": "1.0.0"
  }
}
