{
  "expo": {
    "name": "Muziek voor Kinderopvang",
    "slug": "muziek-voor-kinderopvang",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "bundleIdentifier": "com.tuacompany.muziekvoorkinderopvang",
      "supportsTablet": true,
      "buildNumber": "1.0.0"
    },

    "android": {
      "package": "com.tuacompany.muziekvoorkinderopvang",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },

    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],

    "experiments": {
      "typedRoutes": true
    }
  }
}
